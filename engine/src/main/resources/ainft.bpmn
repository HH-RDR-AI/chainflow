    <bpmn:scriptTask id="Activity_0imoc1t" name="Set, substract retries" scriptFormat="JavaScript">
      <bpmn:incoming>Flow_0pfd8ft</bpmn:incoming>
      <bpmn:outgoing>Flow_17kzja3</bpmn:outgoing>
      <bpmn:script>var retries = execution.getVariable('retries');

// Check if retries is not defined or not a number
if (retries === null || typeof retries !== 'number') {
  // Set retries to 1 if it is not set or not a number
  retries = 1;
} else {
  // Ensure retries is only decremented if it's greater than 0
  if (retries &gt; 0) {
    retries -= 1;
  }
}

// Update the retries variable in the execution context
execution.setVariable('retries', retries);

print('Retries set to: ' + retries);</bpmn:script>
    </bpmn:scriptTask>
